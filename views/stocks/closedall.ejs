            <li><a href=''>Log Out</a></li>
          </ul>
          <ul class='side-nav' id='mobile-demo'>
            <li><a href='/users/new'>Sign Up</a></li>
            <li><a href='/token/login'>Log In</a></li>
            <li><a href='#'>Log Out</a></li>
          </ul>

          <ul class='tabs tabs-transparent'>
            <li class='tab'><a href='/'>Home</a></li>
            <li class='tab'><a href='/transactions/new'>Trade</a></li>
            <li class='tab'><a href='/positions/open'>Open Positions</a></li>
            <li class='tab'><a href='/positions/closed'>Closed Positions</a></li>
          </ul>
        </div>
      </nav>
    </div>

    <main>
      <!-- TC2000 Chart BEGIN -->
      <div class="TC2000-container">
        <div class="row">

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29641"></iframe>
          </div>

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29646"></iframe>
          </div>

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29737"></iframe>
          </div>

        </div>
      </div>
      <!-- TC2000 Chart END -->

      <!-- Open Position Table BEGIN -->
      <div class="container">
        <table>
          <thead>
            <tr>
              <th data-field="buyDate">Buy Date</th>
              <th data-field="sellDate">Sell Date</th>
              <th data-field="ticker">Symbol</th>
              <th data-field="shares">Shares</th>
              <th data-field="buyAmount">Buy Amount</th>
              <th data-field="sellAmount">Sell Amount</th>
              <th data-field="glAmount">$Trade G/L</th>
              <th data-field="glInPercent">%Trade G/L</th>
              <th data-field="FolioAmount">$Account G/L</th>
              <th data-field="FolioAmountPercent">%Account G/L/</th>
            </tr>
          </thead>

          <tbody>
            <% calcArr.forEach(function(trade){ %>
              <tr>
                <td class="class_buyDate"><%=trade.buyDate%></td>
                <td class="class_sellDate"><%=trade.sellDate%></td>
                <td class="class_ticker"><%=trade.ticker%></td>
                <td class="class_shares"><%=trade.shares%></td>
                <td class="class_buyAmount"><%=trade.buyAmount%></td>
                <td class="class_sellAmount"><%=trade.sellAmount%></td>
                <td class="class_glAmount"><%=trade.glAmount%></td>
                <td class="class_glInPercent"><%=trade.glInPercent%></td>
                <td class="class_FolioAmount"><%=trade.FolioAmount%></td>
                <td class="class_FolioAmountPercent"><%=trade.FolioAmountPercent%></td>
              </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
      <!-- Open Position Table BEGIN -->

    </main>

    <footer class='page-footer'>
      <div class='container'>
        <div class='row'>
          <div class='col l6 s12'>
          <!-- <h5 class='white-text'>Footer Content</h5> -->
            <p class='grey-text text-lighten-4'>You can use rows and columns here to organize your footer content.</p>
          </div>
          <div class='col l4 offset-l2 s12'>
          <!-- <h5 class='white-text'>Links</h5> -->
            <ul>
              <li><a class='grey-text text-lighten-3' href='/site/contact'>Contact</a></li>
              <li><a class='grey-text text-lighten-3' href='/site/about'>About</a></li>
            </ul>
          </div>
        </div>
      </div>
      <div class='footer-copyright'>
        <div class='container'>
