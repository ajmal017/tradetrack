<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="../css/materialize.css"  media="screen,projection"/>
    <link rel="stylesheet" href="../css/style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>All</title>
  </head>

    <%- include("../partials/nav", {home: false, trade: false, open: true, closed: false}) %>

    <main>
      <!-- TC2000 Chart BEGIN -->
      <div class="TC2000-container">
        <div class="row">

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29641"></iframe>
          </div>

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29646"></iframe>
          </div>

          <div class="col s12 m4 l4">
            <iframe class="iframe-class" scrolling="no" frameborder="0" src="https://widgets.tc2000.com/WidgetServer.ashx?id=29737"></iframe>
          </div>

        </div>
      </div>
      <!-- TC2000 Chart END -->

      <!-- Open Position Table BEGIN -->
      <div class="container stock-table">
        <table>
          <thead>
            <tr>
              <th data-field="ticker">Symbol</th>
              <th data-field="name">Company</th>
              <th data-field="numShares">Shares</th>
              <th data-field="sharePrice">Orig</th>
              <th data-field="previousClose">Prev</th>
              <th data-field="lastTradePriceOnly">Last</th>
              <th data-field="val">Value</th>
              <th data-field="change">$Change</th>
              <th data-field="changeInPercent">%Change</th>
              <th data-field="totalChange">$G/L</th>
              <th data-field="totalChangeInPercent">%G/L</th>
              <th data-field="peRatio">P/E</th>
              <th data-field="dividendYield">Div Yield</th>
            </tr>
          </thead>

          <tbody>
            <% openPositions.forEach(function(pos){ %>
              <tr>
                <td class="class_ticker"><%=pos.ticker%></td>
                <td class="class_name"><%=pos.name%></td>
                <td class="class_numShares"><%=pos.numShares%></td>
                <td class="class_sharePrice"><%=pos.sharePrice%></td>
                <td class="class_previousClose"><%=pos.previousClose%></td>
                <td class="class_lastTradePriceOnly"><%=pos.lastTradePriceOnly%></td>
                <td class="class_val"><%=pos.val%></td>
                <td class="class_change"><%=pos.change%></td>
                <td class="class_changeInPercent"><%=pos.changeInPercent%></td>
                <td class="class_glDollar"><%=pos.glDollar%></td>
                <td class="class_glInPercent"><%=pos.glInPercent%></td>
                <td class="class_peRatio"><%=pos.peRatio%></td>
                <td class="class_dividendYield"><%=pos.dividendYield%></td>
              </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
      <!-- Open Position Table BEGIN -->

    </main>

    <%- include("../partials/footer") %>

    <script type="text/javascript" src="../js/showall.js"></script>

  </body>
</html>
